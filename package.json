  "name": "EffectNote",
  "version": "0.5.2",
  "private": true,
  "homepage": "/",
  "author": "weiwenda",
  "description": "My Description",
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "@node-rs/jieba": "^1.6.1",
    "cors": "^2.8.5",
    "cos-js-sdk-v5": "^1.4.18",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.16.4",
    "isomorphic-git": "^1.21.0",
